{
    "name": "renatovdemoura/blade-elements-ui",
    "description": "Laravel Blade components using TALL stack.",
    "keywords": ["laravel", "tailwind", "blade", "tall stack", "alpinejs", "livewire"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "Daniel Palmieri",
          "email": "dlspalmieri@gmail.com"
        }
      ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "DanPalmieri\\BladeElementsUi\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanPalmieri\\BladeElementsUi\\BladeElementsUiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
